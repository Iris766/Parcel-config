{
  "name": "parcel-js-demo",
  "version": "1.0.0",
  "description": "Basic parcel Config for Modren JS",
  "main": "index.js",
  "scripts": {
    "dev": "rm -rf ./development ./.cache && parcel public/index.html --out-dir development -p 3000",
    "build": "parcel build public/*.html --out-dir dist --public-url ./",
    "clean": "rimraf ./development && rimraf -rf ./.cache"
  },
  "keywords": [],
  "author": "Muhammad Irtiza",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/runtime-corejs2": "^7.12.1",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.27.0"
  }
}
